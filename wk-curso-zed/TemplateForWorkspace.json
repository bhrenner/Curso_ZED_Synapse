{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"workspaceName": {
			"type": "string",
			"metadata": "Workspace name",
			"defaultValue": "wk-curso-zed"
		},
		"wk-curso-zed-WorkspaceDefaultSqlServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'wk-curso-zed-WorkspaceDefaultSqlServer'"
		},
		"wk-curso-zed-WorkspaceDefaultStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sacursozed.dfs.core.windows.net"
		}
	},
	"variables": {
		"workspaceId": "[concat('Microsoft.Synapse/workspaces/', parameters('workspaceName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('workspaceName'), '/wk-curso-zed-WorkspaceDefaultSqlServer')]",
			"type": "Microsoft.Synapse/workspaces/linkedServices",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"parameters": {
					"DBName": {
						"type": "String"
					}
				},
				"annotations": [],
				"type": "AzureSqlDW",
				"typeProperties": {
					"connectionString": "[parameters('wk-curso-zed-WorkspaceDefaultSqlServer_connectionString')]"
				},
				"connectVia": {
					"referenceName": "AutoResolveIntegrationRuntime",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('workspaceId'), '/integrationRuntimes/AutoResolveIntegrationRuntime')]"
			]
		},
		{
			"name": "[concat(parameters('workspaceName'), '/wk-curso-zed-WorkspaceDefaultStorage')]",
			"type": "Microsoft.Synapse/workspaces/linkedServices",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "[parameters('wk-curso-zed-WorkspaceDefaultStorage_properties_typeProperties_url')]"
				},
				"connectVia": {
					"referenceName": "AutoResolveIntegrationRuntime",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('workspaceId'), '/integrationRuntimes/AutoResolveIntegrationRuntime')]"
			]
		},
		{
			"name": "[concat(parameters('workspaceName'), '/AutoResolveIntegrationRuntime')]",
			"type": "Microsoft.Synapse/workspaces/integrationRuntimes",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"type": "Managed",
				"typeProperties": {
					"computeProperties": {
						"location": "AutoResolve",
						"dataFlowProperties": {
							"computeType": "General",
							"coreCount": 8,
							"timeToLive": 0
						}
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('workspaceName'), '/WorkspaceSystemIdentity')]",
			"type": "Microsoft.Synapse/workspaces/credentials",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"type": "ManagedIdentity",
				"typeProperties": {}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('workspaceName'), '/CreateDimEmployee')]",
			"type": "Microsoft.Synapse/workspaces/sqlscripts",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"folder": {
					"name": "pl_sql_zed_scripts"
				},
				"content": {
					"query": "CREATE TABLE [dbo].[DimEmployee](\n[EmployeeKey] [int] NOT NULL,\n[ParentEmployeeKey] [int] NULL,\n[EmployeeNationalIDAlternateKey] [nvarchar](15) NULL,\n[ParentEmployeeNationalIDAlternateKey] [nvarchar](15) NULL,\n[SalesTerritoryKey] [int] NULL,\n[FirstName] [nvarchar](50) NOT NULL,\n[LastName] [nvarchar](50) NOT NULL,\n[MiddleName] [nvarchar](50) NULL,\n[NameStyle] [bit] NOT NULL,\n[Title] [nvarchar](50) NULL,\n[HireDate] [date] NULL,\n[BirthDate] [date] NULL,\n[LoginID] [nvarchar](256) NULL,\n[EmailAddress] [nvarchar](50) NULL,\n[Phone] [nvarchar](25) NULL,\n[MaritalStatus] [nchar](1) NULL,\n[EmergencyContactName] [nvarchar](50) NULL,\n[EmergencyContactPhone] [nvarchar](25) NULL,\n[SalariedFlag] [bit] NULL,\n[Gender] [nchar](1) NULL,\n[PayFrequency] [tinyint] NULL,\n[BaseRate] [money] NULL,\n[VacationHours] [smallint] NULL,\n[SickLeaveHours] [smallint] NULL,\n[CurrentFlag] [bit] NOT NULL,\n[SalesPersonFlag] [bit] NOT NULL,\n[DepartmentName] [nvarchar](50) NULL,\n[StartDate] [date] NULL,\n[EndDate] [date] NULL,\n[Status] [nvarchar](50) NULL,\n[EmployeePhoto] [varbinary](max) NULL\n)\nWITH\n(\nDISTRIBUTION = REPLICATE,\nCLUSTERED INDEX (EmployeeKey)\n);\nGO\n",
					"metadata": {
						"language": "sql"
					},
					"currentConnection": {
						"databaseName": "pl_sql_zed",
						"poolName": "pl_sql_zed"
					},
					"resultLimit": 5000
				},
				"type": "SqlQuery"
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('workspaceName'), '/InsertDimEmployee')]",
			"type": "Microsoft.Synapse/workspaces/sqlscripts",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"folder": {
					"name": "pl_sql_zed_scripts"
				},
				"content": {
					"query": "COPY INTO [dbo].[DimEmployee]\nFROM 'https://solliancepublicdata.blob.core.windows.net/dataengineering/dp-203/awdata/DimEmployee.csv'\nWITH (\n    FILE_TYPE='CSV',\n    FIELDTERMINATOR='|',\n    FIELDQUOTE='',\n    ROWTERMINATOR='\\n',\n    ENCODING = 'UTF16'\n);\nGO\n",
					"metadata": {
						"language": "sql"
					},
					"currentConnection": {
						"databaseName": "pl_sql_zed",
						"poolName": "pl_sql_zed"
					},
					"resultLimit": 5000
				},
				"type": "SqlQuery"
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('workspaceName'), '/SelectCSVExterno')]",
			"type": "Microsoft.Synapse/workspaces/sqlscripts",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"folder": {
					"name": "Serverless Pool SQL"
				},
				"content": {
					"query": "SELECT *\n\nFROM OPENROWSET(\n    BULK 'https://pandemicdatalake.blob.core.windows.net/public/curated/covid-19/ecdc_cases/latest/ecdc_cases.csv',\n    FORMAT = 'CSV',\n    PARSER_VERSION = '2.0',\n    HEADER_ROW = TRUE\n    ) as [r]\n\n    WHERE [countries_and_territories] = 'Brazil'",
					"metadata": {
						"language": "sql"
					},
					"currentConnection": {
						"databaseName": "master",
						"poolName": "Built-in"
					},
					"resultLimit": 5000
				},
				"type": "SqlQuery"
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('workspaceName'), '/poolzedSpark')]",
			"type": "Microsoft.Synapse/workspaces/bigDataPools",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"autoPause": {
					"enabled": true,
					"delayInMinutes": 10
				},
				"autoScale": {
					"enabled": true,
					"maxNodeCount": 3,
					"minNodeCount": 3
				},
				"nodeCount": 0,
				"nodeSize": "Small",
				"nodeSizeFamily": "MemoryOptimized",
				"sparkVersion": "2.4",
				"isComputeIsolationEnabled": false,
				"sessionLevelPackagesEnabled": true,
				"annotations": []
			},
			"dependsOn": [],
			"location": "eastus"
		},
		{
			"name": "[concat(parameters('workspaceName'), '/pl_sql_zed')]",
			"type": "Microsoft.Synapse/workspaces/sqlPools",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"collation": "SQL_Latin1_General_CP1_CI_AS",
				"maxSizeBytes": 263882790666240,
				"annotations": []
			},
			"dependsOn": [],
			"location": "eastus"
		},
		{
			"name": "[concat(parameters('workspaceName'), '/CriarDataFrame')]",
			"type": "Microsoft.Synapse/workspaces/notebooks",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"nbformat": 4,
				"nbformat_minor": 2,
				"bigDataPool": {
					"referenceName": "poolzedSpark",
					"type": "BigDataPoolReference"
				},
				"sessionProperties": {
					"driverMemory": "28g",
					"driverCores": 4,
					"executorMemory": "28g",
					"executorCores": 4,
					"numExecutors": 2,
					"conf": {
						"spark.dynamicAllocation.enabled": "false",
						"spark.dynamicAllocation.minExecutors": "2",
						"spark.dynamicAllocation.maxExecutors": "2",
						"spark.autotune.trackingId": "1fe36b13-b1af-4393-ba4a-94128359ab9f"
					}
				},
				"metadata": {
					"saveOutput": true,
					"enableDebugMode": false,
					"kernelspec": {
						"name": "synapse_pyspark",
						"display_name": "Synapse PySpark"
					},
					"language_info": {
						"name": "python"
					},
					"a365ComputeOptions": {
						"id": "/subscriptions/d3d24f13-d7d1-426d-bdb6-b822471f5f12/resourceGroups/rg-curso-zero-ao-engenheiro/providers/Microsoft.Synapse/workspaces/wk-curso-zed/bigDataPools/poolzedSpark",
						"name": "poolzedSpark",
						"type": "Spark",
						"endpoint": "https://wk-curso-zed.dev.azuresynapse.net/livyApi/versions/2019-11-01-preview/sparkPools/poolzedSpark",
						"auth": {
							"type": "AAD",
							"authResource": "https://dev.azuresynapse.net"
						},
						"sparkVersion": "2.4",
						"nodeCount": 3,
						"cores": 4,
						"memory": 28,
						"automaticScaleJobs": false
					},
					"sessionKeepAliveTimeout": 30
				},
				"cells": [
					{
						"cell_type": "code",
						"metadata": {},
						"source": [
							"new_tupla1 = ('CA', 10000, 60000)\r\n",
							"new_tupla2 = ('WA', 12450, 55000)\r\n",
							"new_tupla3 = ('LA', 12333, 45000)\r\n",
							"\r\n",
							"new_array_linhas = [new_tupla1, new_tupla2, new_tupla3]\r\n",
							"new_array_colunas = ['State', 'Population', 'Salary']\r\n",
							"\r\n",
							"df = spark.createDataFrame(new_array_linhas, new_array_colunas)\r\n",
							"df.show()\r\n",
							"\r\n",
							"#print(new_array_linhas)"
						],
						"outputs": [],
						"execution_count": 2
					},
					{
						"cell_type": "code",
						"metadata": {
							"jupyter": {
								"source_hidden": false,
								"outputs_hidden": false
							},
							"nteract": {
								"transient": {
									"deleting": false
								}
							}
						},
						"source": [
							"from azureml.opendatasets import NycTlcYellow\r\n",
							"\r\n",
							"data = NycTlcYellow()\r\n",
							"\r\n",
							"df2 = data.to_spark_dataframe()\r\n",
							"\r\n",
							"display(df2.limit(10))"
						],
						"outputs": [],
						"execution_count": 6
					}
				]
			},
			"dependsOn": []
		}
	]
}